<odoo>
	<data>
		<record id="website_pages_form_view_redirect" model="ir.ui.view">
			<field name="name">login_redirect.website_pages_form_view_redirect</field>
			<field name="inherit_id" ref="website.website_pages_form_view" />
			<field name="model">website.page</field>

			<field name="arch" type="xml">
				<xpath expr="//field[@name='url']" position="after">
                    <field name="url_logged"/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>